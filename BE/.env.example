# Database
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/youtube_maker_db

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:5173

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
ACCESS_TOKEN_TTL_SEC=900
REFRESH_TOKEN_TTL_SEC=2592000

# Session Configuration
SESSION_TTL_SEC=2592000

# Cookie Configuration
COOKIE_SECURE=false
COOKIE_DOMAIN=localhost
COOKIE_SAMESITE=lax

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Application
APP_ENV=development

# YouTube Data API
YOUTUBE_API_KEY=your_youtube_api_key_here

# YouTube Subtitle (자막 다운로드)
# true로 설정 시 yt-dlp로 자막 추출 (false면 스킵)
YOUTUBE_SUBTITLE_ENABLED=false
# Cookies 파일 경로 (선택): 브라우저에서 YouTube 로그인 후 쿠키 추출
YOUTUBE_COOKIES_FILE=
# Proxy URL (선택): Webshare.io 등의 프록시 서비스
# 형식: http://username:password@proxy.example.com:port
YOUTUBE_PROXY_URL=


# News Research 이미지 AI 분석 (true로 설정 시 GPT Vision 호출, false면 스킵 → 429 방지)
NEWS_IMAGE_ANALYSIS_ENABLED=false

# 태윤님 api
TAVILY_API_KEY=
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=
GOOGLE_API_KEY=

#도현님 api
NANO_BANANA_API_KEY=

#나경님 api
GEMINI_API_KEY=